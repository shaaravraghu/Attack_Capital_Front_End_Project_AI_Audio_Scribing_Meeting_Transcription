{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "eslint:recommended"
  ],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "ignoreCase": true,
        "callbacksLast": false,
        "shorthandFirst": true,
        "shorthandLast": false,
        "noSortAlphabetically": false,
        "reservedFirst": true
      }
    ],
    "@next/next/no-img-element": "off"
  }
}


